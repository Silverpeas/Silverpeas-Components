<ConnecteurJDBC-configuration version="$id">
	<param>
	      <param-name>Drivers</param-name>
	      <param-description>list of available drivers</param-description>
	      <param-value>StandardJDBCDriver</param-value>
	      <param-value>StandardJDBCODBCDriver</param-value>
	 </param>


<StandardJDBCDriver-configuration>
	   <param>
	      <param-name>DriverName</param-name>
	      <param-description>name of the driver</param-description>
	      <param-value>Standard JDBC driver</param-value>
	   </param>
	   <param>
	      <param-name>ClassName</param-name>
	      <param-description>class name of the driver</param-description>
	      <param-value>org.postgresql.Driver</param-value>
	   </param>
	   <param>
	      <param-name>Description</param-name>
	      <param-description>description of the driver</param-description>
	      <param-value>Standard JDBC driver</param-value>
	   </param>
	   <param>
	      <param-name>JDBCUrls</param-name>
	      <param-description>connexion urls</param-description>
	      <param-value>jdbc:postgresql://localhost:5432/Silverpeas</param-value>
	   </param>
	   
</StandardJDBCDriver-configuration>

<StandardJDBCODBCDriver-configuration>
	   <param>
	      <param-name>DriverName</param-name>
	      <param-description>name of the driver</param-description>
	      <param-value>Standard JDBC-0DBC driver</param-value>
	   </param>
	   <param>
	      <param-name>ClassName</param-name>
	      <param-description>class name of the driver</param-description>
	      <param-value>org.postgresql.Driver</param-value>
	   </param>
	   <param>
	      <param-name>Description</param-name>
	      <param-description>description of the driver</param-description>
	      <param-value>Standard JDBC-ODBC driver</param-value>
	   </param>

	   <param>
	      <param-name>JDBCUrls</param-name>
	      <param-description>connexion urls</param-description>
	      <param-value>jdbc:postgresql://localhost:5432/Silverpeas</param-value>
	   </param>
</StandardJDBCODBCDriver-configuration>

</ConnecteurJDBC-configuration >