<web-app>
  <display-name>Almanach War</display-name>

  <servlet>
    <display-name>AlmanachRequestRouter</display-name>
    <servlet-name>AlmanachRequestRouter</servlet-name>
    <servlet-class>org.silverpeas.core.web.mvc.webcomponent.WebComponentRequestRouter</servlet-class>
    <init-param>
      <param-name>WebComponentController</param-name>
      <param-value>org.silverpeas.components.almanach.AlmanachWebController</param-value>
    </init-param>
  </servlet>
  <servlet>
    <display-name>GoToEvent</display-name>
    <servlet-name>GoToEvent</servlet-name>
    <servlet-class>org.silverpeas.components.almanach.GoToEvent</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AlmanachRequestRouter</servlet-name>
    <url-pattern>/Ralmanach/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GoToEvent</servlet-name>
    <url-pattern>/Event/*</url-pattern>
  </servlet-mapping>
</web-app>

