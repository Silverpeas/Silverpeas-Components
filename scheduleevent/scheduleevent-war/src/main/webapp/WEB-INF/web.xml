<web-app version="2.4">
  <display-name>scheduleevent War</display-name>
  <description>no description</description>
  <servlet>
    <servlet-name>ScheduleEventRequestRouter</servlet-name>
    <display-name>ScheduleEventRequestRouter</display-name>
    <description>no description</description>
    <servlet-class>com.silverpeas.scheduleevent.servlets.ScheduleEventRequestRouter</servlet-class>
  </servlet>
  <servlet>
    <display-name>GoToScheduleEvent</display-name>
    <servlet-name>GoToScheduleEvent</servlet-name>
    <servlet-class>com.silverpeas.scheduleevent.servlets.GoToScheduleEvent</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ScheduleEventRequestRouter</servlet-name>
    <url-pattern>/Rscheduleevent/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GoToScheduleEvent</servlet-name>
    <url-pattern>/ScheduleEvent/*</url-pattern>
  </servlet-mapping>
</web-app>
